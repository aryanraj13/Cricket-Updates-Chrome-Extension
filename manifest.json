{
    "name": "My Chrome Extension",
    "version": "1.0.0",
    "description": "Best extension in the world",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "images/icons8-cricket-64.png",
            "48": "images/icons8-cricket-64.png",
            "128": "images/icons8-cricket-64.png"
        },
        "default_popup": "index.html"
    },
    "icons": {
        "16": "images/icons8-cricket-64.png",
        "48": "images/icons8-cricket-64.png",
        "128": "images/icons8-cricket-64.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; style-src 'self' 'unsafe-inline'; object-src 'self';"
    }
}
